{
    "name": "mjphaynes/php-resque",
    "type": "library",
    "description": "Redis backed library for creating background jobs and processing them later.",
    "keywords": [
        "job",
        "background",
        "redis",
        "resque",
        "php",
        "php-resque",
        "queue",
        "worker"
    ],
    "homepage": "https://github.com/mjphaynes/php-resque/",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Haynes",
            "email": "mike@mjphaynes.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-pcntl": "*",
        "predis/predis": "1.1.*",
        "monolog/monolog": "^2.5",
        "symfony/console": "^5.4|^6.0",
        "symfony/yaml": "^5.4|^6.0"
    },
    "suggest": {
        "ext-phpiredis": "Native PHP extension for Redis connectivity. Predis will automatically utilize when available.",
        "symfony/process": "To run the speed test command."
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/process": "^5.4|^6.0"
    },
    "bin": [
        "bin/resque"
    ],
    "scripts": {
        "cs-ci": "tools/php-cs-fixer fix --verbose",
        "cs-fix": "tools/php-cs-fixer fix --verbose",
        "test": "phpunit"
    },
    "autoload": {
        "psr-4": {
            "Resque\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    }
}
